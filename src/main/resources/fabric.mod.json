{
	"schemaVersion": 1,
	"id": "rubiesmod",
	"version": "${version}",
	"name": "Rubies Mod",
	"description": "Minecraft mod which adds rubies and related items.",
	"authors": [
		"MechaMessiah"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/rubiesmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.mecha.rubiesmod.RubiesMod"
		],
		"fabric-datagen": [
			"net.mecha.rubiesmod.RubiesModDataGenerator"
		],
		"client": [
			"net.mecha.rubiesmod.RubiesModClient"
		]
	},
	"mixins": [
		"rubies-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
